<log4r_config>
  <pre-config>
    <global level="DEBUG"/>
  </pre-config>

  <outputter name="stdout" type="StdoutOutputter">
  </outputter>
  <outputter name="Evernote" type="EvernoteOutputter">
    <env>sandbox</env>
    <notebook>Log</notebook>
    <stack>Last.fm</stack>
    <auth_token>xxxxxxxxxxxxxxxxxxxxxx</auth_token>
    <formatter type="PatternFormatter" pattern="%d %C[%l]: %M " date_format="%Y/%m/%d %H:%M:%S"/>
  </outputter>

  <logger name="test">
    <outputter>stdout</outputter>
    <outputter>Evernote</outputter>
  </logger>
</log4r_config>
